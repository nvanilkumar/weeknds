<h1>Listing users</h1>

<table>
  <tr>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>Email</th>
    <th>Password digest</th>
    <th>Phone</th>
    <th>Mobile</th>
    <th>Isfbuser</th>
    <th>Isgrouporganiser</th>
    <th>Photourl</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.firstname %></td>
    <td><%= user.lastname %></td>
    <td><%= user.email %></td>
    <td><%= user.password_digest %></td>
    <td><%= user.phone %></td>
    <td><%= user.mobile %></td>
    <td><%= user.isFBUser %></td>
    <td><%= user.isGroupOrganiser %></td>
    <td><%= user.photourl %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

